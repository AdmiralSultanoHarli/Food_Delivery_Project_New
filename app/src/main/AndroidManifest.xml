<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fooddeliveryproject">

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.Activity.MenuScreenActivity"></activity>
        <activity android:name=".Activities.JustForTest.TestingActivity" />
        <activity android:name=".Activities.Activity.MenuDetailsScreenActivity" />
        <activity
            android:name=".Activities.Activity.HomeScreenActivity"
            android:label="@string/title_activity_home_screen"
            android:theme="@style/BlackStatusBar" />
        <activity
            android:name=".Activities.Activity.LoginScreenActivity"
            android:theme="@style/BlackStatusBar" />
        <activity
            android:name=".Activities.Activity.SignUpScreenActivity"
            android:theme="@style/BlackStatusBar" />
        <activity
            android:name=".Activities.Activity.AppScreenActivity"
            android:theme="@style/AppStatusBar" />
        <activity android:name=".Activities.Activity.SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>